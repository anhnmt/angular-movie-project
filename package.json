    "docker.compose": "docker-compose up --build -d",
    "docker.local": "npm run docker.build && npm run docker.push",
    "docker.server": "cross-var docker build -t $npm_package_docker_server/$npm_package_name:$npm_package_version . && cross-var docker push $npm_package_docker_server/$npm_package_name:$npm_package_version"
